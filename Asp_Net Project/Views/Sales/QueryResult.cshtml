@model IEnumerable<Asp_Net_Project.Models.InserViewModel>

@{
    ViewBag.Title = "QueryResult";
}

<h2>查詢結果</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OrderID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShippedDate)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Freight)
        </th>
        <th></th>
    </tr>

@foreach (var item in ViewBag.List) {
    <tr>
        <td>
            @item.OrderID
        </td>
        <td>
            @item.ContactName
        </td>
        <td>
            @item.OrderDate.ToString("yyyy-MM-dd")
        </td>
        <td>
            @item.ShippedDate.ToString("yyyy-MM-dd")
        </td>
        <td>
            @item.Freight.ToString("N0")
        </td>
        <td>
            @Html.ActionLink("Edit", "EditOrders", new { id= item.OrderID }) |
            @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
        </td>
    </tr>
}

</table>

<p>
    @Html.ActionLink("回查詢頁", "Index")
</p>
